{
  "name": "oneview-react-azure",
  "version": "1.0.2",
  "main": "server.js",
  "scripts": {
    "start": "npm run build:fast && node server.js",
    "build": "npm run build:frontend && npm run install:api",
    "build:frontend": "vite build",
    "build:fast": "npm run build:frontend && npm run install:api",
    "install:api": "cd api && npm install --production --no-audit --no-fund",
    "postinstall": "npm run install:api"
  },
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "mssql": "^11.0.1",
    "axios": "^1.10.0",
    "vite": "^5.4.0",
    "@vitejs/plugin-react": "^4.3.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.263.1",
    "recharts": "^2.8.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "terser": "^5.19.0"
  }
}
